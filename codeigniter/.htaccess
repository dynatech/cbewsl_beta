	RewriteEngine on
	RewriteCond $1 !^(index\.php|assets|images|temp|css|fonts|js|ajax|d3graph|phpmyadmin|robots\.txt)
	#RewriteCond %{REQUEST_FILENAME} !-f
	#RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule ^(.*)$ /index.php/$1 [L]
	php_value upload_max_filesize 700M
	php_value post_max_size 16M
	<IfModule mod_headers.c>
        Header set Access-Control-Allow-Origin "http://192.168.150.10:5000/"
	
	</IfModule>
